{
  "buildCommand": "node build.js",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/api/auth", "destination": "/api/auth.js" }
  ],
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
